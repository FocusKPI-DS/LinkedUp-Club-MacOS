          "uploadDebugSymbols": false,
          "fileOutput": "macos/Runner/GoogleService-Info.plist"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "linkedup-c3e29",
          "configurations": {
            "android": "1:548534727055:android:f3f3d5491d9b0fb8bb5bfa",
            "ios": "1:548534727055:ios:8343a86672d8b349bb5bfa",
            "macos": "1:548534727055:ios:8343a86672d8b349bb5bfa",
            "web": "1:548534727055:web:d770e39d4c066094bb5bfa"
          }
